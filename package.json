{
    "name": "autofile",
    "private": true,
    "version": "0.1.0",
    "packageManager": "pnpm@8.15.4",
    "scripts": {
        "build": "pnpm -r build",
        "dev": "pnpm -w --filter @autofile/web dev",
        "lint": "pnpm -r lint",
        "test": "vitest -w",
        "typecheck": "pnpm -r typecheck",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "seed": "tsx scripts/seed.ts"
    },
    "devDependencies": {
        "@types/node": "^20.12.12",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.5",
        "prettier": "^3.3.3",
        "prisma": "^5.17.0",
        "tsx": "^4.16.2",
        "typescript": "^5.4.5",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@prisma/client": "^5.17.0"
    }
}